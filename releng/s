tree -aL 3 ~/github/CintyaOS/releng

cat ~/github/CintyaOS/releng/packages.x86_64

cat ~/github/CintyaOS/releng/customize_airootfs.sh

cat ~/github/CintyaOS/releng/airootfs/etc/passwd
cat ~/github/CintyaOS/releng/airootfs/etc/shadow | grep -E '^(root|cintya):'
cat ~/github/CintyaOS/releng/airootfs/etc/group | grep wheel

cat ~/github/CintyaOS/releng/airootfs/etc/sudoers.d/wheel 2>/dev/null || echo "file gaada"

cat ~/github/CintyaOS/releng/airootfs/etc/systemd/system/getty@tty1.service.d/override.conf 2>/dev/null || echo "gaada override.conf"

cat ~/github/CintyaOS/releng/airootfs/home/cintya/.bash_profile 2>/dev/null || echo "gaada .bash_profile"
cat ~/github/CintyaOS/releng/airootfs/home/cintya/.zprofile 2>/dev/null || echo "gaada .zprofile"

du -sh ~/github/CintyaOS/out/*
ls -lh ~/github/CintyaOS/out/ | grep .iso

